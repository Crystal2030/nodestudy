<%- include header %>
<% posts.forEach(function(post, index){ %>
<p><h2><a href="/blog/<%= post.name%>/<%= post.time.day %>/<%= post.title%>"></a></h2></p>
<p class="info">
    Author: <a href="/blog/<%= post.name%>"><%= post.name %></a> |
    Date: <%= post.time.minute %>ï½œ
    Tag:
    <% post.tags.forEach(function(tag, index){ %>
    <% if(tag){ %>
    <a href="/tags/<%= tag %>" class="tag"><%= tag %></a>
    <% } %>
    <% }) %>
</p>
<p><%- post.post %></p>
<% }) %>
<%- include pagination %>
<%- include footer %>